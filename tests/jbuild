(jbuild_version 1)

(executables
 ((names (test))
  (flags (:standard -w +a-4-6-30-32-39-41-42-44-48-60 -warn-error +a))
  (ocamlopt_flags (:standard -w +a-4-6-30-32-39-41-42-44-48-60 -warn-error +a -O3))
  (libraries (tt))))

(install
 ((section bin)
  (files ((test.exe as test)))))

(alias
 ((name runtest)
  (deps (test.exe))
  (action (run ${<}))))
